{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "github": {
        "silent": true
    },
    "rewrites": [{"source": "/(.*)", "destination": "/api"}],
    "redirects": [
        {
            "source": "/",
            "destination": "https://notapi.js.cool",
            "permanent": true
        }
    ]
}
